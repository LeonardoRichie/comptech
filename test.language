#print
PRINT("PRINT HELLO WORLD:")
PRINT("HELLO WORLD")
PRINT("")

#variable
PRINT("Variables:")
VAR C = 3
PRINT("")

#if statement
PRINT("If Statement:")
IF C == 3 THEN PRINT("YES ITS 3")
PRINT(C)
PRINT("")

#function
PRINT("Function:")
FUN add(a,b) -> PRINT(a+b)
add(5,3)
PRINT("")

#loop for
PRINT("Loop:")
VAR result = 1
FOR i = 3 TO 0 STEP -1 THEN VAR result = result + 1
PRINT (result)
PRINT("")

#While
PRINT("While:")
VAR x = 0
WHILE x < 100 THEN VAR x = x+5
PRINT(x)
PRINT("")

#list
PRINT("List:")
VAR list = [3,5,6]
list + 1
PRINT(list)
PRINT("")